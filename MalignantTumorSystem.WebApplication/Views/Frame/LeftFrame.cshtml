        .menu_hide {
            background: url(/images/main/hide.gif) right no-repeat;
            background-color: #0066cc;
            color: white;
            font-size: 14px;
            height: 26px;
            border: 2px solid #84bf92;
            padding-left: 13px;
            cursor: pointer;
